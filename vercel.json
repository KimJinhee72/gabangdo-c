{
  "rewrites": [
    {
      "source": "/(en|ko)/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    },
    {
      "builds": [{ "src": "src/main.js", "use": "@vercel/node" }],
      "routes": [{ "src": "/(.*)", "dest": "src/main.js" }]
    }
  ]
}
